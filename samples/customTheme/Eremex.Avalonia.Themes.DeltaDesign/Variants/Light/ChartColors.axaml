<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Color x:Key="Stroke/Strong/Color">#ff385252</Color>
	<Color x:Key="Stroke/Medium/Color">#ffcadada</Color>
	<Color x:Key="Stroke/Light/Color">#ffdbe6e6</Color>
	<Color x:Key="Background/Chart/Color">#ffffffff</Color>
	<Color x:Key="Background/DrawingArea/Color">#fff7f9f9</Color>
	<Color x:Key="Interlacing/Color">#58dbe6e6</Color>
	<Color x:Key="Crosshair/BackColor">#ffa17ec8</Color>
	<Color x:Key="Crosshair/TextColor">#ffffffff</Color>
    <Color x:Key="Selection/Color">#40808080</Color>
    
    <SolidColorBrush x:Key="Border/Chart" Color="{DynamicResource Stroke/Medium/Color}" />
    <SolidColorBrush x:Key="Border/Chart/DrawingArea" Color="{DynamicResource Stroke/Light/Color}" />
    <SolidColorBrush x:Key="Border/CrosshairSeriesLabel" Color="{DynamicResource Stroke/Medium/Color}" />
    <SolidColorBrush x:Key="Fill/Chart" Color="{DynamicResource Background/Chart/Color}" />
    <SolidColorBrush x:Key="Fill/Chart/DrawingArea" Color="{DynamicResource Background/DrawingArea/Color}" />
    <SolidColorBrush x:Key="Fill/CrosshairSeriesLabel" Color="{DynamicResource Background/Chart/Color}" />
</ResourceDictionary>
