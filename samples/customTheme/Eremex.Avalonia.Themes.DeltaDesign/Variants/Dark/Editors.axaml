<ResourceDictionary xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:sys="using:System">
    
    <BoxShadows x:Key="PopupBoxShadow">0 0 0 0 #00000000</BoxShadows>
    <Thickness x:Key="PopupBoxShadowMargin">0,2,0,2</Thickness>
    <sys:Double x:Key="PopupHorizontalOffset">0</sys:Double>
    <StaticResource x:Key="PopupBorderBrush" ResourceKey="Outline/Neutral/Transparent/Strong"/>
    <StaticResource x:Key="PopupBorderBrush/NoShadow" ResourceKey="Outline/Neutral/Transparent/Strong"/>
</ResourceDictionary>
